<?xml version="1.0"?>
<extension key="aip" type="module">
  <file>aip</file>
  <name>Automated Input Processing</name>
  <description>Allows you to configure various automated processors for different input types</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>B. Endres</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/aip</url>
    <url desc="Documentation">https://github.com/systopia/aip</url>
    <url desc="Support">https://github.com/systopia/aip</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-11-22</releaseDate>
  <version>1.2.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.50</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Aip</namespace>
    <format>23.02.1</format>
    <angularModule>crmAip</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>afform-entity-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Aip_Upgrader</upgrader>
</extension>
